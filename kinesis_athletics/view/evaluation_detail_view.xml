<!-- kinesis_athletics.evaluation_detail -->
<!-- kinesis_athletics.evaluation_detail -->
<!-- kinesis_athletics.evaluation_detail -->
<openerp>
    <data>

        <!-- SEARCH VIEW -->
        <record id="view_kinesis_athletics_evaluation_detail_filter" model="ir.ui.view">
            <field name="name">kinesis_athletics.evaluation_detail.select</field>
            <field name="model">kinesis_athletics.evaluation_detail</field>
            <field name="arch" type="xml">
                <search string="evaluation_detail" version="7.0">
                    <group string="By Attribute">
                    <field name="result" string="Result"
                            />
                    </group>
                    <group string="By object">
                    <field name="test_id" string="Test"
                            />
                    </group>
                </search>
            </field>
        </record>

        <!-- FORMVIEW -->
        <record id="view_kinesis_athletics_evaluation_detail_form" model="ir.ui.view">
            <field name="name">kinesis_athletics.evaluation_detail.form</field>
            <field name="model">kinesis_athletics.evaluation_detail</field>
            <field name="arch" type="xml">
                <form string="evaluation_detail" version="7.0">
                    <header>
                    </header>
                    <sheet string="evaluation_detail">
                     <group>
                        <field name="result"
                            />
                    <field name="test_id"
                            />
                     </group>
                    </sheet>

                </form>
            </field>
        </record>

        <!-- TREEVIEW -->

        <record id="view_kinesis_athletics_evaluation_detail_tree" model="ir.ui.view">
            <field name="name">kinesis_athletics.evaluation_detail.tree</field>
            <field name="model">kinesis_athletics.evaluation_detail</field>
            <field name="arch" type="xml">
                <tree string="evaluation_detail"
                    >
                    <field name="result"
                        />
                    <field name="evaluation_id"
                        />
                    <field name="test_id"
                        />
                </tree>
            </field>
        </record>

    </data>
</openerp>
