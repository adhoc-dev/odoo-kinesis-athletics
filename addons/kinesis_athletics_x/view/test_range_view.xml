            <field name="model">kinesis_athletics.test_range</field>
            <field name="inherit_id" ref="kinesis_athletics.view_kinesis_athletics_test_range_tree"/>
            <field name="arch" type="xml">
                <tree position="replace">
                <tree string="Test Range" editable="top">
                    <field name="from_age"/>
                    <field name="to_age"/>
                    <field name="sex"/>
                    <field name="extreme_minimum"/>
                    <field name="val_min"/>
                    <field name="val_max"/>
                    <field name="extreme_maximum"/>
                </tree>
                </tree>
            </field>
        </record>

    </data>
</openerp>
