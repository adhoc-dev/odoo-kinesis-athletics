<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>
        <template id="report_evaluation">
            <t t-call="report.html_container">
                <t t-call="report.external_layout">
                    <div class="page">
                        <t t-foreach="docs" t-as="o">
                            <div class="row">
                                <div class="col-xs-4">
                                    <h3 t-field="o.name"/>
                                </div>
                            </div>
                        </t>
                    </div>
                </t>
            </t>
        </template>
    </data>
</openerp>
