<!-- kinesis_athletics.test_selection -->
<!-- kinesis_athletics.test_selection -->
<!-- kinesis_athletics.test_selection -->
<openerp>
    <data>

        <!-- FORMVIEW -->
        <record id="view_kinesis_athletics_test_selection_form" model="ir.ui.view">
            <field name="name">kinesis_athletics.test_selection.form</field>
            <field name="model">kinesis_athletics.test_selection</field>
            <field name="inherit_id" ref="kinesis_athletics.view_kinesis_athletics_test_selection_form"/>
            <field name="arch" type="xml">
                    <sheet position="replace">
                    <sheet>
                        <field name="image" widget="image" class="oe_left oe_avatar" options="{'size': [90, 90]}"/>
                        <div class="oe_title">
                            <div class="oe_edit_only">
                                <label for="name"/>
                        </div>
                            <h1>
                                <field name="name" default_focus="1" placeholder="Name"/>
                            </h1>
                        </div>
                    </sheet>
                    </sheet>
            </field>
        </record>

    </data>
</openerp>
